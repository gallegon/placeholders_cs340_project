<script src="deleteEvent.js"></script>
<h3>Events:</h3>
<table class = "table">
    <thead>
        <tr> 
            <th scope="col">Event Name</th> 
            <th scope="col">Date/Time</th> 
            <th scope="col">City</th>
            <th scope="col">Country</th>
        </tr>
    </thead> 
    <tbody>
        {{#each events}} 
        <tr> 
            <th scope="row">{{eventName}}</td> 
            <td>{{dateTime}}</td> 
            <td>{{city}}</td>
            <td>{{country}}</td>
            <td><button class = "btn btn-outline-danger" onclick="return deleteEvent({{eventID}})">Delete</button></td>
            <td><a href="">update</a></td>
        </tr> 
        {{/each}} 
    </tbody> 
</table>